//Fecha hacia abajo
.DownArrow
	border-color verde transparent verde transparent
	border-style solid
	border-width .9em .9em 0 .9em 
	cursor pointer
	display block
	float right
	height 0
	margin 0 .5em 0 0
	right 0
	width 0

.UpArrow
	border-color verde transparent verde transparent
	border-style solid
	border-width 0em .9em .9em .9em 
	cursor pointer
	display block
	float right
	height 0
	margin .18em .5em 0 0
	right 0
	width 0

.cssSlideUp
	transition .4s linear all
	height 33em
	overflow hidden
	&.ng-hide
		height 0

.cssSlideUp--Maestra
	transition .4s linear all
	height 36rem
	overflow hidden
	&.ng-hide
		height 0
		
.cssFade
	transition .5s
	opacity 1
	&.ng-hide
		opacity 0
		margin-top 30em
	

.cssSlideUpError
	background #FFCDBF
	border-bottom 1px solid rojoOscuro
	color red
	height 5em
	left 0
	overflow hidden
	padding .3em 0 0 .3em
	position fixed
	right 0
	top 0
	transition .3s linear all
	z-index 99999
	& span
		float left
	
	&.ng-hide
		height 0
		
.cerrarError
	text-decoration none
	background rojoOscuro
	color white
	border-radius 50%
	padding .3em .6em
	float right
	margin 0 .2em 0 0
	&:hover
		background red

.errorMessage
	font-size 1.4em
	font-weight bold
	line-height 20px
	padding-top 1em
	margin-left .5em
		
.errorMessageIcon
	
	&:before
		font-size 4.5em

